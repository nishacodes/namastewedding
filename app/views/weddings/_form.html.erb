<%= simple_form_for(@wedding) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :url %>
    <%= f.input :bridename %>
    <%= f.input :groomname %>
    <%= f.input :weddingdate %>
    <%= f.input :weddingcity %>
    <%= f.input :weddingstate %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
